{"ast":null,"code":"import axios from 'axios';\nimport HeaderComponent from '@/components/HeaderComponent.vue';\nimport FooterComponent from '@/components/FooterComponent.vue';\nimport info from '@/views/Oficios.json';\nexport default {\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      anuncios: []\n    };\n  },\n  mounted() {\n    axios.get('/anuncios.json').then(response => {\n      this.anuncios = response.data;\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"names":["axios","HeaderComponent","FooterComponent","info","components","data","anuncios","mounted","get","then","response","catch","error","console","log"],"sources":["/home/ubuntu/development/code/Proyecto Chambeaya/src/views/PostAdView.vue"],"sourcesContent":["<template>\n\n<div>\n        <h1>Publicación de anuncio</h1>\n        <form @submit.prevent=\"publicarAnuncio\">\n\n            <div>\n                <h2>Anuncios</h2>\n                <ul>\n                    <li v-for=\"(anuncio, index) in anuncios\" :key=\"index\">\n                        <h3>{{ anuncio.nickname }}</h3>\n                        <p>Código postal: {{ anuncio.postalCode }}</p>\n                        <p>Oficio: {{ anuncio.occupation }}</p>\n                        <p>Teléfono: {{ anuncio.phone }}</p>\n                        <p>Correo electrónico: {{ anuncio.email }}</p>\n                    </li>\n                </ul>\n            </div>\n\n\n            <label for=\"fotos\">Fotos:</label>\n            <input type=\"file\" id=\"fotos\" v-on:change=\"subirFotos\" multiple>\n\n            <label for=\"videos\">Videos:</label>\n            <input type=\"file\" id=\"videos\" v-on:change=\"subirVideos\" multiple>\n\n            <label for=\"descripcion\">Breve descripción:</label>\n            <textarea id=\"descripcion\" v-model=\"anuncio.descripcion\" required></textarea>\n\n            <label for=\"palabras-clave\">Palabras clave:</label>\n            <input type=\"text\" id=\"palabras-clave\" v-model=\"anuncio.palabrasClave\">\n\n            <label for=\"descripcion-amplia\">Descripción amplia:</label>\n            <textarea id=\"descripcion-amplia\" v-model=\"anuncio.descripcionAmplia\" @input=\"contarPalabras\"\n                maxlength=\"2000\"></textarea>\n            <p>{{ contadorPalabras }} / 2000 palabras</p>\n\n            <label for=\"tiempo-publicacion\">Tiempo de publicación:</label>\n            <input type=\"date\" id=\"tiempo-publicacion\" v-model=\"anuncio.tiempoPublicacion\">\n\n            <label for=\"redes-sociales\">Redes sociales:</label>\n            <input type=\"text\" id=\"redes-sociales\" v-model=\"anuncio.redesSociales\">\n\n            <button type=\"submit\">Publicar anuncio</button>\n        </form>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport HeaderComponent from '@/components/HeaderComponent.vue'\nimport FooterComponent from '@/components/FooterComponent.vue';\nimport info from '@/views/Oficios.json';\nexport default {\n    components: {\n        HeaderComponent,\n        FooterComponent\n    },\n\n    data() {\n        return {\n            anuncios: []\n        }\n    },\n    mounted() {\n        axios.get('/anuncios.json')\n            .then(response => {\n                this.anuncios = response.data;\n            })\n            .catch(error => {\n                console.log(error);\n            });\n    }\n}\n</script>\n"],"mappings":"AAiDA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,eAAc,MAAO,kCAAiC;AAC7D,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,IAAG,MAAO,sBAAsB;AACvC,eAAe;EACXC,UAAU,EAAE;IACRH,eAAe;IACfC;EACJ,CAAC;EAEDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNP,KAAK,CAACQ,GAAG,CAAC,gBAAgB,EACrBC,IAAI,CAACC,QAAO,IAAK;MACd,IAAI,CAACJ,QAAO,GAAII,QAAQ,CAACL,IAAI;IACjC,CAAC,EACAM,KAAK,CAACC,KAAI,IAAK;MACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;IACtB,CAAC,CAAC;EACV;AACJ"},"metadata":{},"sourceType":"module","externa