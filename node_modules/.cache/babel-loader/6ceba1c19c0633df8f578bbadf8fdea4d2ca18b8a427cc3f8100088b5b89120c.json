{"ast":null,"code":"import info from \"@/views/Datos.json\";\nimport HeaderComponent from \"@/components/HeaderComponent.vue\";\nimport FooterComponent from \"@/components/FooterComponent.vue\";\nexport default {\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      anuncios: info.anuncios\n    };\n  },\n  computed: {\n    anunciosFiltrados() {\n      if (this.filtro === \"\") {\n        return this.anuncios;\n      } else {\n        return this.anuncios.filter(anuncio => anuncio.occupation.toLowerCase() === this.filtro.toLowerCase());\n      }\n    },\n    contadorPalabras() {\n      return this.anuncio.descripcionAmplia.split(\" \").length;\n    }\n  },\n  methods: {}\n};","map":{"version":3,"names":["info","HeaderComponent","FooterComponent","components","data","anuncios","computed","anunciosFiltrados","filtro","filter","anuncio","occupation","toLowerCase","contadorPalabras","descripcionAmplia","split","length","methods"],"sources":["/home/ubuntu/development/code/Proyecto Chambeaya/src/views/PostAdView.vue"],"sourcesContent":["<template>\n  <div>\n    <HeaderComponent />\n    <div>\n      <h1>Publicación de anuncio</h1>\n      <form>\n        <div>\n          <ul>\n            <li v-for=\"(anuncio, index) in anunciosFiltrados\" :key=\"index\">\n              <p>Nickname: {{ anuncio.nickname }}</p>\n              <p>Oficio: {{ anuncio.occupation }}</p>\n              <p>Teléfono: {{ anuncio.phone }}</p>\n              <p>Código postal: {{ anuncio.postalCode }}</p>\n              <p>Correo electrónico: {{ anuncio.email }}</p>\n            </li>\n          </ul>\n        </div>\n\n        <label for=\"descripcion\">Breve descripción:</label>\n        <textarea id=\"descripcion\" v-model=\"anuncio.descripcion\" required></textarea>\n\n        <label for=\"palabras-clave\">Palabras clave:</label>\n        <input type=\"text\" id=\"palabras-clave\" v-model=\"anuncio.palabrasClave\">\n\n        <label for=\"descripcion-amplia\">Descripción amplia:</label>\n        <textarea id=\"descripcion-amplia\" v-model=\"anuncio.descripcionAmplia\" @input=\"contarPalabras\"\n          maxlength=\"2000\"></textarea>\n        \n\n        <label for=\"tiempo-publicacion\">Tiempo de publicación:</label>\n        <input type=\"date\" id=\"tiempo-publicacion\" v-model=\"anuncio.tiempoPublicacion\">\n\n        <label for=\"redes-sociales\">Redes sociales:</label>\n        <input type=\"text\" id=\"redes-sociales\" v-model=\"anuncio.redesSociales\">\n\n        <button type=\"submit\">Publicar anuncio</button>\n      </form>\n    </div>\n    <FooterComponent />\n  </div>\n</template>\n  \n<script>\nimport info from \"@/views/Datos.json\";\nimport HeaderComponent from \"@/components/HeaderComponent.vue\";\nimport FooterComponent from \"@/components/FooterComponent.vue\";\n\nexport default {\n  components: {\n    HeaderComponent,\n    FooterComponent,\n  },\n\n  data() {\n    return {\n      anuncios: info.anuncios,\n\n    };\n  },\n\n  computed: {\n    anunciosFiltrados() {\n      if (this.filtro === \"\") {\n        return this.anuncios;\n      } else {\n        return this.anuncios.filter(\n          (anuncio) => anuncio.occupation.toLowerCase() === this.filtro.toLowerCase()\n        );\n      }\n    },\n    contadorPalabras() {\n      return this.anuncio.descripcionAmplia.split(\" \").length;\n    },\n  },\n\n  methods: {\n\n  },\n};\n</script>\n  "],"mappings":"AA2CA,OAAOA,IAAG,MAAO,oBAAoB;AACrC,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,eAAc,MAAO,kCAAkC;AAE9D,eAAe;EACbC,UAAU,EAAE;IACVF,eAAe;IACfC;EACF,CAAC;EAEDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAEL,IAAI,CAACK;IAEjB,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACRC,iBAAiBA,CAAA,EAAG;MAClB,IAAI,IAAI,CAACC,MAAK,KAAM,EAAE,EAAE;QACtB,OAAO,IAAI,CAACH,QAAQ;MACtB,OAAO;QACL,OAAO,IAAI,CAACA,QAAQ,CAACI,MAAM,CACxBC,OAAO,IAAKA,OAAO,CAACC,UAAU,CAACC,WAAW,EAAC,KAAM,IAAI,CAACJ,MAAM,CAACI,WAAW,EAAC,CAC3E;MACH;IACF,CAAC;IACDC,gBAAgBA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACH,OAAO,CAACI,iBAAiB,CA