d=\"numero-cuenta\" name=\"numero-cuenta\" v-model=\"numeroCuenta\" pattern=\"[0-9]{16}\" required>\n            <small>Debe tener 16 dígitos.</small>\n\n            <label for=\"fecha-vencimiento\">Fecha de vencimiento:</label>\n            <input type=\"text\" id=\"fecha-vencimiento\" name=\"fecha-vencimiento\" v-model=\"fechaVencimiento\"\n                placeholder=\"dd/aa\" pattern=\"[0-9]{2}/[0-9]{2}\" required>\n            <small>Debe tener el formato dd/aa.</small>\n\n            <label for=\"codigo-verificacion\">Código de verificación:</label>\n            <input type=\"text\" id=\"codigo-verificacion\" name=\"codigo-verificacion\" v-model=\"codigoVerificacion\"\n                pattern=\"[0-9]{3}\" required>\n            <small>Debe tener 3 dígitos.</small>\n\n            <div>\n                <input type=\"checkbox\" id=\"terminos\" name=\"terminos\" v-model=\"aceptaTerminos\" required>\n                <label for=\"terminos\">Acepto los términos y condiciones</label>\n                <a href=\"/terminos-y-condiciones\">Leer términos y condiciones</a>\n            </div>\n\n            <button type=\"submit\">Continuar</button>\n        </form>\n    </div>\n    <FooterComponent />\n</template>\n\n<script>\nimport HeaderComponent from '@/components/HeaderComponent.vue';\nimport FooterComponent from '@/components/FooterComponent.vue';\n\nexport default {\n    components: {\n        HeaderComponent,\n        FooterComponent,\n    },\n    data() {\n        return {\n            nombreTitular: '',\n            numeroCuenta: '',\n            fechaVencimiento: '',\n            codigoVerificacion: '',\n            aceptaTerminos: false\n        }\n    },\n    methods: {\n        submitForm() {\n            // Validar si los campos estan llenos\n            if (!this.nombreTitular || !this.numeroCuenta || !this.fechaVencimiento || !this.codigoVerificacion || !this.aceptaTerminos) {\n                alert('Por favor complete todos los campos y acepte los términos y condiciones para continuar.')\n            } else {\n                // Aquí puedes agregar la lógica para enviar los datos a tu servidor o almacenarlos en tu base de datos.\n                console.log(`Titular: ${this.nombreTitular}, Número de cuenta: ${this.numeroCuenta}, Fecha de vencimiento: ${this.fechaVencimiento}, Código de verificación: ${this.codigoVerificacion}, Acepta términos: ${this.aceptaTerminos}`);\n            }\n        }\n    }\n}\n</script>\n  \n<style scoped>\nform {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n}\n\ninput,\nlabel {\n    margin: 5px 0;\n}\n\nsmall {\n    color: red;\n}\n\nbutton {\n    margin-top: 10px;\n}\n\nfooter {\n    margin-top: 30px;\n\n\n    /* body {\n    background-image: url('../assets/oficios.jpg');\n    background-size: cover;\n    position: relative;\n} */\n}</style>"],"mappings":"AAqCA,OAAOA,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,eAAc,MAAO,kCAAkC;AAE9D,eAAe;EACXC,UAAU,EAAE;IACRF,eAAe;IACfC;EACJ,CAAC;EACDE,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,aAAa,EAAE,EAAE;MACjBC,YAAY,EAAE,EAAE;MAChBC,gBAAgB,EAAE,EAAE;MACpBC,kBAAkB,EAAE,EAAE;MACtBC,cAAc,EAAE;IACpB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACT;MACA,IAAI,CAAC,IAAI,CAACN,aAAY,IAAK,CAAC,IAAI,CAACC,YAAW,IAAK,CAAC,IAAI,CAACC,gBAAe,IAAK,CAAC,IAAI,CAACC,kBAAiB,IAAK,CAAC,IAAI,CAACC,cAAc,EAAE;QACzHG,KAAK,CAAC,yFAAyF;MACnG,OAAO;QACH;QACAC,OAAO,CAACC,GAAG,CAAE,YAAW,IAAI,CAACT,aAAc,uBAAsB,IAAI,CAACC,YAAa,2BAA0B,IAAI,CAACC,gBAAiB,6BAA4B,IAAI,CAACC,kBAAmB,sBAAqB,IAAI,CAACC,cAAe,EAAC,CAAC;MACtO;IACJ;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}