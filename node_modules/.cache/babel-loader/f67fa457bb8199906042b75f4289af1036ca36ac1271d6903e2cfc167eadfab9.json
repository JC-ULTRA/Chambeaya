         <input type=\"text\" id=\"palabras-clave\" v-model=\"anuncio.palabrasClave\">\n\n            <label for=\"descripcion-amplia\">Descripción amplia:</label>\n            <textarea id=\"descripcion-amplia\" v-model=\"anuncio.descripcionAmplia\" @input=\"contarPalabras\"\n                maxlength=\"2000\"></textarea>\n            <p>{{ contadorPalabras }} / 2000 palabras</p>\n\n            <label for=\"tiempo-publicacion\">Tiempo de publicación:</label>\n            <input type=\"date\" id=\"tiempo-publicacion\" v-model=\"anuncio.tiempoPublicacion\">\n\n            <label for=\"redes-sociales\">Redes sociales:</label>\n            <input type=\"text\" id=\"redes-sociales\" v-model=\"anuncio.redesSociales\">\n\n            <button type=\"submit\">Publicar anuncio</button>\n        </form>\n    </div>\n    <FooterComponent />\n</template>\n\n<script>\nimport info from '@/views/'\nimport HeaderComponent from '@/components/HeaderComponent.vue';\nimport FooterComponent from '@/components/FooterComponent.vue';\n\nexport default {\n    components: {\n        HeaderComponent,\n        FooterComponent\n    },\n\n    data() {\n        return {\n            anuncios: []\n        }\n    },\n    mounted() {\n        axios.get('/Datos.json')\n            .then(response => {\n                this.anuncios = response.data.anuncios;\n            })\n            .catch(error => {\n                console.log(error);\n            });\n    }\n}\n</script>\n"],"mappings":"AAmDA,OAAOA,IAAG,MAAO,UAAS;AAC1B,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,eAAc,MAAO,kCAAkC;AAE9D,eAAe;EACXC,UAAU,EAAE;IACRF,eAAe;IACfC;EACJ,CAAC;EAEDE,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNC,KAAK,CAACC,GAAG,CAAC,aAAa,EAClBC,IAAI,CAACC,QAAO,IAAK;MACd,IAAI,CAACL,QAAO,GAAIK,QAAQ,CAACN,IAAI,CAACC,QAAQ;IAC1C,CAAC,EACAM,KAAK,CAACC,KAAI,IAAK;MACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;IACtB,CAAC,CAAC;EACV;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}