{"ast":null,"code":"import HeaderComponent from \"@/components/HeaderComponent.vue\";\nimport FooterComponent from \"@/components/FooterComponent.vue\";\nimport anunciosData from \"@/data/anuncios.json\";\nexport default {\n  name: \"Anuncios\",\n  components: {\n    HeaderComponent,\n    FooterComponent\n  },\n  data() {\n    return {\n      anuncios: anunciosData.anuncios,\n      filtroNumero: null\n    };\n  },\n  computed: {\n    anunciosFiltrados() {\n      if (this.filtroNumero !== null) {\n        return this.anuncios.filter(anuncio => anuncio.postalCode.startsWith(this.filtroNumero.toString()));\n      } else {\n        return this.anuncios;\n      }\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","FooterComponent","anunciosData","name","components","data","anuncios","filtroNumero","computed","anunciosFiltrados","filter","anuncio","postalCode","startsWith","toString"],"sources":["/home/ubuntu/development/code/Proyecto Chambeaya/src/views/PostAdView.vue"],"sourcesContent":["<template>\n  <div>\n    <HeaderComponent />\n    <div>\n      <h1>Publicación de anuncio</h1>\n      <form>\n        <div>\n          <h2>Anuncios</h2>\n          <label for=\"filtro-numero\">Filtrar por número de código postal (1 al 5):</label>\n          <input type=\"number\" id=\"filtro-numero\" v-model.number=\"filtroNumero\">\n\n          <ul>\n            <li v-for=\"(anuncio, index) in anunciosFiltrados\" :key=\"index\">\n              <h3>{{ anuncio.nickname }}</h3>\n              <p>Código postal: {{ anuncio.postalCode }}</p>\n              <p>Oficio: {{ anuncio.occupation }}</p>\n              <p>Teléfono: {{ anuncio.phone }}</p>\n              <p>Correo electrónico: {{ anuncio.email }}</p>\n            </li>\n          </ul>\n        </div>\n\n        <label for=\"fotos\">Fotos:</label>\n        <input type=\"file\" id=\"fotos\" v-on:change=\"subirFotos\" multiple>\n\n        <label for=\"videos\">Videos:</label>\n        <input type=\"file\" id=\"videos\" v-on:change=\"subirVideos\" multiple>\n\n        <label for=\"descripcion\">Breve descripción:</label>\n        <textarea id=\"descripcion\" v-model=\"anuncio.descripcion\" required></textarea>\n\n        <label for=\"palabras-clave\">Palabras clave:</label>\n        <input type=\"text\" id=\"palabras-clave\" v-model=\"anuncio.palabrasClave\">\n\n        <label for=\"descripcion-amplia\">Descripción amplia:</label>\n        <textarea id=\"descripcion-amplia\" v-model=\"anuncio.descripcionAmplia\" @input=\"contarPalabras\"\n          maxlength=\"2000\"></textarea>\n        <p>{{ contadorPalabras }} / 2000 palabras</p>\n\n        <label for=\"tiempo-publicacion\">Tiempo de publicación:</label>\n        <input type=\"date\" id=\"tiempo-publicacion\" v-model=\"anuncio.tiempoPublicacion\">\n\n        <label for=\"redes-sociales\">Redes sociales:</label>\n        <input type=\"text\" id=\"redes-sociales\" v-model=\"anuncio.redesSociales\">\n\n        <button type=\"submit\">Publicar anuncio</button>\n      </form>\n    </div>\n    <FooterComponent />\n  </div>\n</template>\n\n<script>\nimport HeaderComponent from \"@/components/HeaderComponent.vue\";\nimport FooterComponent from \"@/components/FooterComponent.vue\";\nimport anunciosData from \"@/data/anuncios.json\";\n\nexport default {\n  name: \"Anuncios\",\n  components: {\n    HeaderComponent,\n    FooterComponent,\n  },\n  data() {\n    return {\n      anuncios: anunciosData.anuncios,\n      filtroNumero: null,\n    };\n  },\n  computed: {\n    anunciosFiltrados() {\n      if (this.filtroNumero !== null) {\n        return this.anuncios.filter(\n          (anuncio) =>\n            anuncio.postalCode.startsWith(this.filtroNumero.toString())\n        );\n      } else {\n        return this.anuncios;\n      }\n    },\n  },\n};\n</script>\n"],"mappings":"AAqDA,OAAOA,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,YAAW,MAAO,sBAAsB;AAE/C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVJ,eAAe;IACfC;EACF,CAAC;EACDI,IAAIA,