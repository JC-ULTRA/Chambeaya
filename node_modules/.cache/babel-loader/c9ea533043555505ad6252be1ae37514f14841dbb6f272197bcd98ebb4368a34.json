.get(\"http://localhost:8080/hola/cambia\", {\n                    headers: {tokenDado: store.state.userData.jwt2},\n                    params: {nuevoPassword: 'super-secreto'}\n            })\n            .then(response => {\n                console.log(response.data)\n            })\n            .catch( error => console.log(error))    \n        }\n    }\n}\n</script>"],"mappings":"AAQA,OAAOA,KAAI,MAAO,SAAS;AAC3B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,SAAAA,CAAA,EAAW;IACb,OAAO;MACHC,UAAU,EAAE;IAChB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,KAAK,EAAE,SAAAA,CAAA,EAAW;MACdJ,KAAK,CAACK,GAAG,CAAC,kCAAkC,EAAE;QACtCC,MAAM,EAAE;UAACC,IAAI,EAAE,MAAM;UAAEC,QAAQ,EAAE;QAAO;MAChD,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QACdX,KAAK,CAACY,MAAM,CAAC,SAAS,EAACD,QAAQ,CAACT,IAAI;QACpCW,OAAO,CAACC,GAAG,CAACH,QAAQ,CAACT,IAAI;MAC7B,CAAC,EACAa,KAAK,CAAEC,KAAI,IAAKH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;IACvC,CAAC;IACDC,MAAM,EAAE,SAAAA,CAAA,EAAW;MACfhB,KAAK,CAACK,GAAG,CAAC,mCAAmC,EAAE;QACvCY,OAAO,EAAE;UAACC,SAAS,EAAEnB,KAAK,CAACoB,KAAK,CAACC,QAAQ,CAACC;QAAI,CAAC;QAC/Cf,MAAM,EAAE;UAACgB,aAAa,EAAE;QAAe;MAC/C,CAAC,EACAb,IAAI,CAACC,QAAO,IAAK;QACdE,OAAO,CAACC,GAAG,CAACH,QAAQ,CAACT,IAAI;MAC7B,CAAC,EACAa,KAAK,CAAEC,KAAI,IAAKH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;IACvC;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}