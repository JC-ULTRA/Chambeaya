{"ast":null,"code":"import axios from 'axios';\nimport HeaderComponent from '@/components/HeaderComponent.vue';\nimport FooterComponent from '@/components/FooterComponent.vue';\nimport SideBarComponent from '@/components/SideBarComponent.vue';\nexport default {\n  components: {\n    HeaderComponent,\n    FooterComponent,\n    SideBarComponent\n  },\n  data() {\n    return {\n      imagenes: [],\n      maxImagesAllowed: 2,\n      loadedImages: 0,\n      jwt: ''\n    };\n  },\n  mounted() {\n    this.obtenerImagenes();\n  },\n  methods: {\n    subirIne(cargarIne) {\n      this.$refs[cargarIne + 'INE'].click();\n    },\n    obtenerImagenes() {\n      axios.get('https://upload.qbits.mx/api/up/get-user-identification-images/52', {\n        headers: {\n          'Authorization': `Bearer ${this.jwt}`\n        }\n      }).then(response => {\n        this.imagenes = response.data;\n        this.loadedImages = this.imagenes.length;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    getImagenUrl(fullHttpUploadUrl) {\n      return `https://media.visitanos.net/image${fullHttpUploadUrl}`;\n    },\n    recuperarJWT() {\n      axios.post('https://access.qbits.mx/api/login', {\n        email: 'fadota2967@mitigado.com',\n        password: 'TU_CONTRASEÑA'\n      }).then(response => {\n        this.jwt = response.data.jwt;\n        localStorage.setItem('jwt', this.jwt);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    eliminarImagen(id) {\n      axios.delete(`https://upload.qbits.mx/api/up/delete-media/${id}`, {\n        headers: {\n          'Authorization': `Bearer ${this.jwt}`,\n          'idUser': 52\n        },\n        data: {\n          idMedia: id\n        }\n      }).then(response => {\n        console.log(response.data);\n        this.obtenerImagenes();\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","HeaderComponent","FooterComponent","SideBarComponent","components","data","imagenes","maxImagesAllowed","loadedImages","jwt","mounted","obtenerImagenes","methods","subirIne","cargarIne","$refs","click","get","headers","then","response","length","catch","error","console","log","getImagenUrl","fullHttpUploadUrl","recuperarJWT","post","email","password","localStorage","setItem","eliminarImagen","id","delete","idMedia"],"sources":["/home/ubuntu/development/code/Proyecto Chambeaya/src/views/OfficialDocUpLoadView.vue"],"sourcesContent":["<template>\n    <body>\n        <div>\n            <HeaderComponent />\n            <div class=\"d-md-block col-md-3\">\n                <SideBarComponent />\n            </div>\n            <main class=\"container pt-5 hoja\">\n\n                <div>\n                    <h1 class=\"text-center mb-5\">Captura de identificación oficial</h1>\n                </div>\n                <form>\n\n                    <div v-if=\"loadedImages < maxImagesAllowed\" class=\"row justify-content-center\">\n                        <div class=\"col-sm-6 col-md-4 mb-4\">\n                            <label>Identificación oficial anverso</label>\n                            <div class=\"mb-3\">\n                                <a href=\"/ui/upload\">\n                                    <img src=\"../assets/anverso.png\" alt=\"Identificación oficial anverso\"\n                                        class=\"img-fluid mb-2 img-thumbnail fa-fade\"\n                                        style=\"--fa-animation-duration: 2s; --fa-fade-opacity: 0.6;  width: 200px; height: 125px; \">\n                                </a>\n                            </div>\n                        </div>\n\n\n                        <div class=\"col-sm-6 col-md-4 mb-4\">\n                            <label>Identificación oficial reverso</label>\n                            <div class=\"mb-3\">\n                                <a href=\"/ui/upload\">\n                                    <img src=\"../assets/reverso.png\" alt=\"Identificación oficial reverso\"\n                                        class=\"img-fluid mb-2 img-thumbnail fa-fade\"\n                                        style=\"--fa-animation-duration: 2s; --fa-fade-opacity: 0.6; width: 200px; height: 125px;\">\n                                </a>\n                            </div>\n                        </div>\n                    </div>\n\n                    <div v-else>\n                        <h1>El máximo de imágenes permitido ha sido alcanzado</h1>\n                    </div>\n\n                    <div class=\"row\">\n                        <div class=\"col-sm-6 col-md-4 mb-4\" v-for=\"imagen in imagenes\" :key=\"imagen.id\">\n                            <img :src=\"getImagenUrl(imagen.fullHttpUploadUrl)\" alt=\"identificación\"\n                                class=\"img-fluid mb-2 img-thumbnail\" style=\"width: 400px; height: 200px;\">\n                            <button @click=\"eliminarImagen(imagen.id)\">Eliminar imagen</button>\n                        </div>\n                    </div>\n\n\n                    <div class=\"d-grid pb-5 mt-5 col-2 mx-auto\">\n                        <button type=\"submit\" class=\"btn btn1\">Enviar</button>\n                    </div>\n                </form>\n\n\n            </main>\n            <FooterComponent />\n        </div>\n    </body>\n</template>\n\n<script>\nimport axios from 'axios';\nimport HeaderComponent from '@/components/HeaderComponent.vue';\nimport FooterComponent from '@/components/FooterComponent.vue';\nimport SideBarComponent from '@/components/SideBarComponent.vue';\n\nexport default {\n  components: {\n    HeaderComponent,\n    FooterComponent,\n    SideBarComponent,\n  },\n  data() {\n    return {\n      imagenes: [],\n      maxImagesAllowed: 2,\n      loadedImages: 0,\n      jwt: ''\n    }\n  },\n  mounted() {\n    this.obtenerImagenes();\n    \n  },\n  methods: {\n    subirIne(cargarIne) {\n      this.$refs[cargarIne + 'INE'].click();\n    },\n    obtenerImagenes() {\n      axios.get('https://upload.qbits.mx/api/up/get-user-identification-images/52', {\n        headers: {\n          'Authorization': `Bearer ${this.jwt}`\n        }\n      })\n        .then(response => {\n          this.imagenes = response.data;\n          this.loadedImages = this.imagenes.length;\n        })\n        .catch(error => {\n          console.log(error);\n        });\n    },\n    getImagenUrl(fullHttpUploadUrl) {\n      return `https://media.visitanos.net/image${fullHttpUploadUrl}`;\n    },\n    recuperarJWT() {\n    axios.post('https://access.qbits.mx/api/login', {\n      email: 'fadota2967@mitigado.com',\n      password: 'TU_CONTRASEÑA'\n    })\n    .then(response => {\n      this.jwt = response.data.jwt;\n      localStorage.setItem('jwt', this.jwt);\n    })\n    .catch(error => {\n      console.log(error);\n    });\n  },\n  eliminarImagen(id) {\n    axios.delete(`https://upload.qbits.mx/api/up/delete-media/${id}`, {\n      headers: {\n        'Authorization': `Bearer ${this.jwt}`,\n        'idUser': 52\n      },\n      data: {\n        idMedia: id\n      }\n    })\n    .then(response => {\n      console.log(response.data);\n      this.obtenerImagenes();\n    })\n    .catch(error => {\n      console.log(error);\n    });\n  }\n  },\n}\n</script>\n\n\n<style scoped>\n.ordenar img {\n    display: block;\n}\n\ndiv.row {\n    justify-content: center;\n    gap: 10%;\n}\n\n.ordenar button {\n    display: block;\n}\n\n.hoja {\n    background: #a5a5a5dc;\n}\n\n.ordenar {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    flex-direction: column;\n    background: whitesmoke;\n    border-radius: 10px;\n}\n\n.btn1 {\n    background: #8BC34A;\n}\n\nh1 {\n    font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;\n    color: black;\n    text-shadow: 5px 0px 3px whitesmoke;\n    ;\n    font-size: 2.5rem;\n    line-height: 1.2;\n}\n\nbody {\n    background-image: url('../assets/oficios.jpg');\n    background-size: cover;\n    position: relative;\n}\n\n#titulo {\n    cursor: pointer;\n    transition: color 0.3s ease-in-out;\n}\n\n#titulo:hover {\n    cursor: crosshair;\n    color: darkblue;\n}\n\n.btn1:hover {\n    color: white;\n}\n</style>"],"mappings":"AAiEA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,gBAAe,MAAO,mCAAmC;AAEhE,eAAe;EACbC,UAAU,EAAE;IACVH,eAAe;IACfC,eAAe;IACfC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,CAAC;MACnBC,YAAY,EAAE,CAAC;MACfC,GAAG,EAAE;IACP;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,EAAE;EAExB,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAACC,SAAS,EAAE;MAClB,IAAI,CAACC,KAAK,CAACD,SAAQ,GAAI,KAAK,CAAC,CAACE,KAAK,EAAE;IACvC,CAAC;IACDL,eAAeA,CAAA,EAAG;MAChBX,KAAK,CAACiB,GAAG,CAAC,kEAAkE,EAAE;QAC5EC,OAAO,EAAE;UACP,eAAe,EAAG,UAAS,IAAI,CAACT,GAAI;QACtC;MACF,CAAC,EACEU,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACd,QAAO,GAAIc,QAAQ,CAACf,IAAI;QAC7B,IAAI,CAACG,YAAW,GAAI,IAAI,CAACF,QAAQ,CAACe,MAAM;MAC1C,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACN,CAAC;IACDG,YAAYA,CAACC,iBAAiB,EAAE;MAC9B,OAAQ,oCAAmCA,iBAAkB,EAAC;IAChE,CAAC;IACDC,YAAYA,CAAA,EAAG;MACf5B,KAAK,CAAC6B,IAAI,CAAC,mCAAmC,EAAE;QAC9CC,KAAK,EAAE,yBAAyB;QAChCC,QAAQ,EAAE;MACZ,CAAC,EACAZ,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACX,GAAE,GAAIW,QAAQ,CAACf,IAAI,CAACI,GAAG;QAC5BuB,YAAY,CAACC,OAAO,CAAC,KAAK,EAAE,IAAI,CAACxB,GAAG,CAAC;MACvC,CAAC,EACAa,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ,CAAC;IACDW,cAAcA,CAACC,EAAE,EAAE;MACjBnC,KAAK,CAACoC,MAAM,CAAE,+CAA8CD,EAAG,EAAC,EAAE;QAChEjB,OAAO,EAAE;UACP,eAAe,EAAG,UAAS,IAAI,CAACT,GAAI,EAAC;UACrC,QAAQ,EAAE;QACZ,CAAC;QACDJ,IAAI,EAAE;UACJgC,OAAO,EAAEF;QACX;MACF,CAAC,EACAhB,IAAI,CAACC,QAAO,IAAK;QAChBI,OAAO,CAACC,GAAG,CAACL,QAAQ,CAACf,IAAI,CAAC;QAC1B,IAAI,CAACM,eAAe,EAAE;MACxB,CAAC,EACAW,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ;EACA;AACF"},"metadata":{},"source